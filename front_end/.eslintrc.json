    "extends": "airbnb",
    "rules": {
        "max-len": ["error", 80],
        "no-underscore-dangle": ["error", { "allow": ["_id"] }],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    },
    "globals": {
        "fetch": false,
        "document": false
        
      }
}